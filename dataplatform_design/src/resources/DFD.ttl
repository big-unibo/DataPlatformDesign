@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix DPDO: <http://www.semanticweb.org/manuele.pasini2/ontologies/2024/4/DPDO#> .
@prefix DFD: <http://www.semanticweb.org/manuele.pasini2/ontologies/2024/4/DFD#> .
@prefix TagTaxonomy: <http://www.semanticweb.org/manuele.pasini2/ontologies/2024/4/TagTaxonomy#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/manuele.pasini2/ontologies/2024/4/DFD> .

<http://www.semanticweb.org/manuele.pasini2/ontologies/2024/4/DFD> rdf:type owl:Ontology ;
    owl:imports <http://www.semanticweb.org/manuele.pasini2/ontologies/2024/4/DPDO>,
                <http://www.semanticweb.org/manuele.pasini2/ontologies/2024/4/TagTaxonomy> .

###  http://www.semanticweb.org/manuele.pasini2/ontologies/2024/4/DPDO#Sensor_Data
DFD:Sensor_Data rdf:type owl:NamedIndividual ,
                      DPDO:Repository ;
             DPDO:flowsData DFD:Data_Enrichment ;
             DPDO:hasTag TagTaxonomy:Relational ,
                         TagTaxonomy:Small ,
                         TagTaxonomy:Vectorial ;
             DPDO:name "Sensor Data" .

###  http://www.semanticweb.org/manuele.pasini2/ontologies/2024/4/DPDO#Raw_Images
DFD:Raw_Images rdf:type owl:NamedIndividual ,
                     DPDO:Repository ;
            DPDO:flowsData DFD:Index_Comp ;
            DPDO:hasTag TagTaxonomy:Archive ,
                        TagTaxonomy:Big ,
                        TagTaxonomy:File ,
                        TagTaxonomy:Landing ,
                        TagTaxonomy:Raster ;
            DPDO:name "Raw Images" .

###  http://www.semanticweb.org/manuele.pasini2/ontologies/2024/4/DPDO#Data_Enrichment
DFD:Data_Enrichment rdf:type owl:NamedIndividual ,
                        DPDO:Process ;
                    DPDO:name "Data Enrichment" .

###  http://www.semanticweb.org/manuele.pasini2/ontologies/2024/4/DPDO#Index_Comp.
DFD:Index_Comp rdf:type owl:NamedIndividual ,
                    DPDO:Process ;
        DPDO:flowsData DFD:Integrated_Data ;
        DPDO:hasTag TagTaxonomy:Batch ,
                    TagTaxonomy:Big ;
        DPDO:name "Index Comp." .
                


###  http://www.semanticweb.org/manuele.pasini2/ontologies/2024/4/DPDO#Integrated_Data
DFD:Integrated_Data rdf:type owl:NamedIndividual ,
                          DPDO:Repository ;
                 DPDO:flowsData DFD:ETL ;
                 DPDO:hasTag TagTaxonomy:Operational ,
                             TagTaxonomy:Relational ,
                             TagTaxonomy:Vectorial ;
                 DPDO:name "Integrated Data." .

###  http://www.semanticweb.org/manuele.pasini2/ontologies/2024/4/DPDO#ETL
DFD:ETL rdf:type owl:NamedIndividual ,
              DPDO:Process ;
            DPDO:flowsData DFD:DWH ;
            DPDO:name "ETL" .

###  http://www.semanticweb.org/manuele.pasini2/ontologies/2024/4/DPDO#DWH
DFD:DWH rdf:type owl:NamedIndividual ,
              DPDO:Repository ;
     DPDO:hasTag TagTaxonomy:Multidimensional ,
                 TagTaxonomy:OLAP ;
    DPDO:name "DWH" .






