ServiceEcosystem:Dataflow
        rdf:type owl:NamedIndividual, DPDO:Service;
        DPDO:hasTag
            TagTaxonomy:Big,
            TagTaxonomy:Batch,
            TagTaxonomy:ETL;
        DPDO:isCompatible
            ServiceEcosystem:BigQuery,
            ServiceEcosystem:Firestore,
            ServiceEcosystem:ComputeEngine,
            ServiceEcosystem:Dataproc,
            ServiceEcosystem:Dataflow,
            ServiceEcosystem:PubSub,
            ServiceEcosystem:GCFunctions,
            ServiceEcosystem:CloudSQL,
            ServiceEcosystem:GCStorage,
            ServiceEcosystem:VertexAI;
        DPDO:name "Dataflow".

ServiceEcosystem:VertexAI
        rdf:type owl:NamedIndividual, DPDO:Service;
        DPDO:hasTag
            TagTaxonomy:Machine_Learning,
            TagTaxonomy:Data_Volume_all,
            TagTaxonomy:Batch;
        DPDO:isCompatible
            ServiceEcosystem:BigQuery,
            ServiceEcosystem:Firestore,
            ServiceEcosystem:ComputeEngine,
            ServiceEcosystem:Dataproc,
            ServiceEcosystem:Dataflow,
            ServiceEcosystem:PubSub,
            ServiceEcosystem:GCFunctions,
            ServiceEcosystem:CloudSQL,
            ServiceEcosystem:GCStorage,
            ServiceEcosystem:VertexAI;
        DPDO:name "VertexAI".

ServiceEcosystem:Dataproc
        rdf:type owl:NamedIndividual, DPDO:Service;
        DPDO:hasTag
            TagTaxonomy:Machine_Learning,
            TagTaxonomy:Big,
            TagTaxonomy:Batch;
        DPDO:requires
            ServiceEcosystem:ComputeEngine;
        DPDO:isCompatible
            ServiceEcosystem:BigQuery,
            ServiceEcosystem:Firestore,
            ServiceEcosystem:ComputeEngine,
            ServiceEcosystem:Dataproc,
            ServiceEcosystem:Dataflow,
            ServiceEcosystem:PubSub,
            ServiceEcosystem:GCFunctions,
            ServiceEcosystem:CloudSQL,
            ServiceEcosystem:GCStorage,
            ServiceEcosystem:VertexAI;
        DPDO:name "Dataproc".

