            ServiceEcosystem:AzureFunctions,
            ServiceEcosystem:AzureSQLDatabase,
            ServiceEcosystem:AzureSynapseAnalytics,
            ServiceEcosystem:AzureBlobStorage,
            ServiceEcosystem:AzureMachineLearning;
        DPDO:name "AzureBlobStorage".

